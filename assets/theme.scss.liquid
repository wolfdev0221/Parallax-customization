          box-shadow: 0 0 0 4px rgba($colorTextBody, 0.25);
        }

        &::-moz-range-track {
          box-shadow: 0 0 0 4px rgba($colorTextBody, 0.25);
        }

        &::-ms-track {
          box-shadow: 0 0 0 4px rgba($colorTextBody, 0.25);
        }
      }

      &:active {
